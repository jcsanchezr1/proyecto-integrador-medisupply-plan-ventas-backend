			"name": "Crear Visita - Datos Válidos",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Definir el rango de fechas",
							"const startDate = new Date(\"2025-12-01T00:00:00Z\");  // Fecha mínima",
							"const endDate = new Date(\"2030-12-01T00:00:00Z\");    // Fecha máxima",
							"",
							"// Calcular un timestamp aleatorio entre ambas fechas",
							"const randomTimestamp = startDate.getTime() + Math.random() * (endDate.getTime() - startDate.getTime());",
							"",
							"// Crear una nueva fecha con ese timestamp",
							"const randomDate = new Date(randomTimestamp);",
							"",
							"// Formatear la fecha a DD-MM-YYYY",
							"const day = String(randomDate.getDate()).padStart(2, '0');",
							"const month = String(randomDate.getMonth() + 1).padStart(2, '0');",
							"const year = randomDate.getFullYear();",
							"const formattedDate = `${day}-${month}-${year}`;",
							"",
							"// Guardar la fecha como variable de entorno",
							"pm.globals.set('randomVisitDate', formattedDate);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Status code is 201', function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"pm.test('Response has success structure', function () {",
							"    const response = pm.response.json();",
							"    pm.expect(response).to.have.property('success');",
							"    pm.expect(response).to.have.property('message');",
							"    pm.expect(response).to.have.property('data');",
							"    pm.expect(response.success).to.be.true;",
							"});",
							"pm.test('Data contains plan information', function () {",
							"    const response = pm.response.json();",
							"    const data = response.data;",
							"    pm.expect(data).to.have.property('id');",
							"    pm.expect(data).to.have.property('seller_id');",
							"    pm.expect(data).to.have.property('date');",
